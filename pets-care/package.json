{
  "name": "pets-care",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "serve": "vue-cli-service serve",
    "build": "vue-cli-service build",
    "lint": "vue-cli-service lint",
    "start-server-dev": "nodemon server/index.js",
    "dev": "npm-run-all --parallel serve start-server-dev",
    "backend": "json-server --watch db/db.json --port 5000"
  },
  "dependencies": {
    "@kouts/vue-modal": "^2.0.1",
    "@linusborg/vue-simple-portal": "^0.1.5",
    "axios": "^0.21.1",
    "bootstrap": "^4.6.0",
    "bootstrap-vue": "^2.21.2",
    "debug": "^4.1.1",
    "express": "^4.16.4",
    "jquery": "^3.6.0",
    "json-server": "^0.16.3",
    "lodash.orderby": "^4.6.0",
    "mdbvue": "^6.7.2",
    "module": "^1.2.5",
    "morgan": "^1.9.1",
    "v-datatable-light": "^0.8.2",
    "vue": "^2.6.12",
    "vue-beautiful-chat": "^2.5.0",
    "vue-dropdowns": "^1.1.2",
    "vue-dynamic-dropdown": "^1.0.12",
    "vue-form-wizard": "^0.8.4",
    "vue-js-grid": "^1.0.1",
    "vue-quick-chat": "^1.2.8",
    "vue-router": "^3.4.9",
    "vue-select": "^3.11.2",
    "vue-simple-alert": "^1.1.1",
    "vue-simple-spinner": "^1.2.10",
    "vuetify": "^1.5.5",
    "vuex": "^3.6.2"
  },
  "devDependencies": {
    "@vue/cli-plugin-babel": "^3.5.0",
    "@vue/cli-plugin-eslint": "^3.5.0",
    "@vue/cli-service": "^3.5.0",
    "babel-eslint": "^10.0.1",
    "eslint": "^5.8.0",
    "eslint-plugin-vue": "^5.0.0",
    "nodemon": "^1.18.10",
    "npm-run-all": "^4.1.5",
    "stylus": "^0.54.5",
    "stylus-loader": "^3.0.1",
    "vue-cli-plugin-vuetify": "^0.5.0",
    "vue-template-compiler": "^2.5.21",
    "vuetify-loader": "^1.0.5"
  },
  "eslintConfig": {
    "root": true,
    "env": {
      "node": true
    },
    "extends": [
      "plugin:vue/essential",
      "eslint:recommended"
    ],
    "rules": {
      "no-console": "off"
    },
    "parserOptions": {
      "parser": "babel-eslint"
    }
  },
  "postcss": {
    "plugins": {
      "autoprefixer": {}
    }
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not ie <= 8"
  ],
  "prettier": {
    "semi": true,
    "singleQuote": false
  }
}
